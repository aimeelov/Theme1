<!doctype html>
<head>
  <title>{{ page_title }}</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="description" content="{{ page_description | escape }}">
  <link rel="canonical" href="{{ canonical_url }}">
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
  {{ content_for_header }} <!-- Header hook for plugins -->
  <link rel="stylesheet" href=" https://cdn.shopify.com/s/files/1/0602/8623/8906/t/scss/_reboot.scss" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
  
  {{ 'bootstrap.min.css' | asset_url | stylesheet_tag }}
  {{ 'application.css.scss' | asset_url | stylesheet_tag }}
  {{ 'font-styles.css' | asset_url | stylesheet_tag }}
  {{ 'theme.css' | asset_url | stylesheet_tag }}
  
</head>
<body>
  {% section 'translation' %}
  {% section 'header' %}

  <main role="main">
    {{ content_for_layout }}
  </main>
  
  {% section 'footer-section' %}
</body>
</html>